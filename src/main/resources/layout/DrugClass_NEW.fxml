                  <Insets bottom="30.0" />
               </VBox.margin>
            </TextField>
            <Label fx:id="abbrLabel" text="Class Abbreviation" />
            <TextField fx:id="inputAbbrField" onKeyPressed="#onClassAbbrEnterKey">
