              </buttons>
               <padding>
                  <Insets right="70.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </ButtonBar>
         </children>
      </VBox>
      <Separator layoutX="397.0" layoutY="2.0" orientation="VERTICAL" prefHeight="310.0" prefWidth="3.0" AnchorPane.bottomAnchor="5.0" AnchorPane.topAnchor="5.0" />
